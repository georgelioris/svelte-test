{
  "homepage": "http://georgelioris.github.io/svelte-test",
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "predeploy": "yarn run rollup -c --environment GH_PAGES",
    "deploy": "gh-pages -d ghbuild",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.18.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.23.2"
  },
  "dependencies": {
    "sirv-cli": "^1.0.3"
  },
  "prettier": {
    "svelteSortOrder": "styles-scripts-markup",
    "svelteStrictMode": true,
    "svelteBracketNewLine": true,
    "svelteAllowShorthand": true,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-svelte"
    ]
  }
}
